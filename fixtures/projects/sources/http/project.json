{
  "name": "http",
  "theme": "stencila",
  "sources": [
    {
      "url": "https://raw.githubusercontent.com/curran/data/gh-pages/Rdatasets/csv/datasets/mtcars.csv",
      "dest": "mtcars.csv"
    },
    {
      "url": "https://httpbin.org/uuid",
      "dest": "httpbin.json",
      "cron": {
        "schedule": "every 30 minutes mon-fri"
      }
    },
    {
      "url": "http://worldtimeapi.org/api/timezone/Pacific/Chatham",
      "dest": "time.json",
      "cron": {
        "schedule": "every 2 hours chatham time"
      }
    }
  ]
}