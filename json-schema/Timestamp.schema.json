{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "https://raw.githubusercontent.com/stencila/stencila/main/json-schema/Timestamp.schema.json",
  "title": "Timestamp",
  "@id": "schema:Timestamp",
  "status": "stable",
  "description": "A value that represents a point in time",
  "properties": {
    "value": {
      "@id": "schema:value",
      "description": "The time, in `timeUnit`s, before or after the Unix Epoch (1970-01-01T00:00:00Z).",
      "type": "integer",
      "from": "Timestamp"
    },
    "timeUnit": {
      "@id": "stencila:timeUnit",
      "description": "The time unit that the `value` represents.",
      "$ref": "TimeUnit.schema.json",
      "from": "Timestamp"
    }
  },
  "required": [
    "value",
    "timeUnit"
  ],
  "source": "https://github.com/stencila/stencila/blob/main/schema/yaml/Timestamp.yaml",
  "children": [],
  "descendants": [],
  "type": "object",
  "additionalProperties": false
