{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "https://raw.githubusercontent.com/stencila/stencila/main/json-schema/Mark.schema.json",
  "title": "Mark",
  "@id": "stencila:Mark",
  "extends": "Entity",
  "status": "stable",
  "description": "A base class for nodes that mark some other inline content\nin some way (e.g. as being emphasised, or quoted).\n",
  "properties": {
    "type": {
      "@id": "schema:type",
      "description": "The name of the type",
      "$comment": "This is a special property analogous to JSON-LD's `@type` keyword.\nIt is extended with the titles of all descendant types during\nthe generation of schema.json files.\n",
      "type": "string",
      "enum": [
        "Mark",
        "Delete",
        "Emphasis",
        "NontextualAnnotation",
        "Quote",
        "Strikeout",
        "Strong",
        "Subscript",
        "Superscript",
        "Underline"
      ],
      "default": "Mark",
      "from": "Entity"
    },
    "id": {
      "@id": "schema:id",
      "description": "The identifier for this item",
      "$comment": "This is a special property analogous to JSON-LD's `@id` keyword.\n",
      "type": "string",
      "from": "Entity"
    },
    "content": {
      "@id": "stencila:content",
      "description": "The content that is marked.",
      "type": "array",
      "items": {
        "$ref": "InlineContent.schema.json"
      },
      "from": "Mark",
      "isArray": true
    }
  },
  "required": [
    "type",
    "content"
  ],
  "source": "https://github.com/stencila/stencila/blob/main/schema/yaml/Mark.yaml",
  "children": [
    "Delete",
    "Emphasis",
    "NontextualAnnotation",
    "Quote",
    "Strikeout",
    "Strong",
    "Subscript",
    "Superscript",
    "Underline"
  ],
  "descendants": [
    "Delete",
    "Emphasis",
    "NontextualAnnotation",
    "Quote",
    "Strikeout",
    "Strong",
    "Subscript",
    "Superscript",
    "Underline"
  ],
  "type": "object",
  "additionalProperties": false,
  "propertyAliases": {}
}