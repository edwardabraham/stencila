{
  "@context": {
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "schema": "https://schema.org/",
    "stencila": "https://stencila.dev/"
  },
  "@graph": [
    {
      "@id": "stencila:QuoteBlock",
      "@type": "rdfs:Class",
      "rdfs:label": "QuoteBlock",
      "rdfs:comment": "A section quoted from somewhere else.\n",
      "rdfs:subClassOf": {
        "@id": "Entity"
      }
    },
    {
      "@id": "stencila:cite",
      "@type": "rdfs:Property",
      "rdfs:label": "cite",
      "rdfs:comment": "The source of the quote.",
      "schema:domainIncludes": [
        {
          "@id": "stencila:Quote"
        },
        {
          "@id": "stencila:QuoteBlock"
        }
      ],
      "schema:rangeIncludes": {
        "@id": "schema:Text"
      }
    },
    {
      "@id": "stencila:content",
      "@type": "rdfs:Property",
      "rdfs:label": "content",
      "rdfs:comment": "The content of the quote.",
      "schema:domainIncludes": [
        {
          "@id": "stencila:Division"
        },
        {
          "@id": "schema:ListItem"
        },
        {
          "@id": "stencila:Link"
        },
        {
          "@id": "stencila:Cite"
        },
        {
          "@id": "stencila:Heading"
        },
        {
          "@id": "stencila:Form"
        },
        {
          "@id": "stencila:Mark"
        },
        {
          "@id": "stencila:Note"
        },
        {
          "@id": "schema:CreativeWork"
        },
        {
          "@id": "stencila:Paragraph"
        },
        {
          "@id": "stencila:For"
        },
        {
          "@id": "stencila:TableCell"
        },
        {
          "@id": "stencila:Include"
        },
        {
          "@id": "schema:Claim"
        },
        {
          "@id": "stencila:Span"
        },
        {
          "@id": "stencila:IfClause"
        },
        {
          "@id": "stencila:QuoteBlock"
        }
      ],
      "schema:rangeIncludes": {
        "@id": "stencila:Block"
      }
    }
  ]
}