{
  "@context": {
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "schema": "https://schema.org/",
    "stencila": "https://stencila.dev/"
  },
  "@graph": [
    {
      "@id": "schema:ListItem",
      "@type": "rdfs:Class",
      "rdfs:label": "ListItem",
      "rdfs:comment": "A single item in a list.",
      "rdfs:subClassOf": {
        "@id": "Thing"
      }
    },
    {
      "@id": "schema:alternateName",
      "@type": "rdfs:Property",
      "rdfs:label": "alternateNames",
      "rdfs:comment": "Alternate names (aliases) for the item.",
      "schema:domainIncludes": {
        "@id": "schema:Thing"
      },
      "schema:rangeIncludes": {
        "@id": "schema:Text"
      }
    },
    {
      "@id": "schema:description",
      "@type": "rdfs:Property",
      "rdfs:label": "description",
      "rdfs:comment": "A description of the item.",
      "schema:domainIncludes": {
        "@id": "schema:Thing"
      },
      "schema:rangeIncludes": {
        "@id": "stencila:Block"
      }
    },
    {
      "@id": "schema:identifier",
      "@type": "rdfs:Property",
      "rdfs:label": "identifiers",
      "rdfs:comment": "Any kind of identifier for any kind of Thing.",
      "schema:domainIncludes": {
        "@id": "schema:Thing"
      },
      "schema:rangeIncludes": {
        "@id": "schema:Text"
      }
    },
    {
      "@id": "schema:image",
      "@type": "rdfs:Property",
      "rdfs:label": "images",
      "rdfs:comment": "Images of the item.",
      "schema:domainIncludes": {
        "@id": "schema:Thing"
      },
      "schema:rangeIncludes": {
        "@id": "schema:Text"
      }
    },
    {
      "@id": "schema:name",
      "@type": "rdfs:Property",
      "rdfs:label": "name",
      "rdfs:comment": "The name of the item.",
      "schema:domainIncludes": [
        {
          "@id": "stencila:Variable"
        },
        {
          "@id": "stencila:Function"
        },
        {
          "@id": "stencila:Button"
        },
        {
          "@id": "stencila:ExecutionTag"
        },
        {
          "@id": "stencila:TableCell"
        },
        {
          "@id": "schema:Thing"
        },
        {
          "@id": "stencila:Parameter"
        }
      ],
      "schema:rangeIncludes": {
        "@id": "schema:Text"
      }
    },
    {
      "@id": "schema:url",
      "@type": "rdfs:Property",
      "rdfs:label": "url",
      "rdfs:comment": "The URL of the item.",
      "schema:domainIncludes": {
        "@id": "schema:Thing"
      },
      "schema:rangeIncludes": {
        "@id": "schema:Text"
      }
    },
    {
      "@id": "stencila:content",
      "@type": "rdfs:Property",
      "rdfs:label": "content",
      "rdfs:comment": "The content of the list item.",
      "schema:domainIncludes": [
        {
          "@id": "stencila:Division"
        },
        {
          "@id": "schema:ListItem"
        },
        {
          "@id": "stencila:Link"
        },
        {
          "@id": "stencila:Cite"
        },
        {
          "@id": "stencila:Heading"
        },
        {
          "@id": "stencila:Form"
        },
        {
          "@id": "stencila:Mark"
        },
        {
          "@id": "stencila:Note"
        },
        {
          "@id": "schema:CreativeWork"
        },
        {
          "@id": "stencila:Paragraph"
        },
        {
          "@id": "stencila:For"
        },
        {
          "@id": "stencila:TableCell"
        },
        {
          "@id": "stencila:Include"
        },
        {
          "@id": "schema:Claim"
        },
        {
          "@id": "stencila:Span"
        },
        {
          "@id": "stencila:IfClause"
        },
        {
          "@id": "stencila:QuoteBlock"
        }
      ],
      "schema:rangeIncludes": [
        {
          "@id": "stencila:Block"
        },
        {
          "@id": "stencila:Inline"
        }
      ]
    },
    {
      "@id": "schema:item",
      "@type": "rdfs:Property",
      "rdfs:label": "item",
      "rdfs:comment": "The item represented by this list item.",
      "schema:domainIncludes": {
        "@id": "schema:ListItem"
      }
    },
    {
      "@id": "stencila:isChecked",
      "@type": "rdfs:Property",
      "rdfs:label": "isChecked",
      "rdfs:comment": "A flag to indicate if this list item is checked.",
      "schema:domainIncludes": {
        "@id": "schema:ListItem"
      },
      "schema:rangeIncludes": {
        "@id": "schema:Boolean"
      }
    },
    {
      "@id": "schema:position",
      "@type": "rdfs:Property",
      "rdfs:label": "position",
      "rdfs:comment": "The position of the item in a series or sequence of items.",
      "schema:domainIncludes": {
        "@id": "schema:ListItem"
      },
      "schema:rangeIncludes": {
        "@id": "schema:Number"
      }
    }
  ]
}