{
  "@context": {
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "schema": "https://schema.org/",
    "stencila": "https://stencila.dev/"
  },
  "@graph": [
    {
      "@id": "stencila:Quote",
      "@type": "rdfs:Class",
      "rdfs:label": "Quote",
      "rdfs:comment": "Inline, quoted content.",
      "rdfs:subClassOf": {
        "@id": "Mark"
      }
    },
    {
      "@id": "stencila:content",
      "@type": "rdfs:Property",
      "rdfs:label": "content",
      "rdfs:comment": "The content that is marked.",
      "schema:domainIncludes": [
        {
          "@id": "schema:Claim"
        },
        {
          "@id": "schema:CreativeWork"
        },
        {
          "@id": "schema:ListItem"
        },
        {
          "@id": "stencila:Cite"
        },
        {
          "@id": "stencila:Division"
        },
        {
          "@id": "stencila:For"
        },
        {
          "@id": "stencila:Form"
        },
        {
          "@id": "stencila:Heading"
        },
        {
          "@id": "stencila:IfClause"
        },
        {
          "@id": "stencila:Include"
        },
        {
          "@id": "stencila:Link"
        },
        {
          "@id": "stencila:Mark"
        },
        {
          "@id": "stencila:Note"
        },
        {
          "@id": "stencila:Paragraph"
        },
        {
          "@id": "stencila:QuoteBlock"
        },
        {
          "@id": "stencila:Span"
        },
        {
          "@id": "stencila:TableCell"
        }
      ],
      "schema:rangeIncludes": {
        "@id": "stencila:Inline"
      }
    },
    {
      "@id": "stencila:cite",
      "@type": "rdfs:Property",
      "rdfs:label": "cite",
      "rdfs:comment": "The source of the quote.",
      "schema:domainIncludes": [
        {
          "@id": "stencila:Quote"
        },
        {
          "@id": "stencila:QuoteBlock"
        }
      ],
      "schema:rangeIncludes": [
        {
          "@id": "Cite"
        },
        {
          "@id": "schema:Text"
        }
      ]
    }
  ]
}