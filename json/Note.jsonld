{
  "@context": {
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "schema": "https://schema.org/",
    "stencila": "https://stencila.dev/"
  },
  "@graph": [
    {
      "@id": "stencila:Note",
      "@type": "rdfs:Class",
      "rdfs:label": "Note",
      "rdfs:comment": "Additional content which is not part of the main content of a document.",
      "rdfs:subClassOf": {
        "@id": "Entity"
      }
    },
    {
      "@id": "stencila:noteType",
      "@type": "rdfs:Property",
      "rdfs:label": "noteType",
      "rdfs:comment": "Determines where the note content is displayed within the document.",
      "schema:domainIncludes": {
        "@id": "stencila:Note"
      }
    },
    {
      "@id": "stencila:content",
      "@type": "rdfs:Property",
      "rdfs:label": "content",
      "rdfs:comment": "Content of the note, usually a paragraph.",
      "schema:domainIncludes": [
        {
          "@id": "stencila:Include"
        },
        {
          "@id": "stencila:Paragraph"
        },
        {
          "@id": "stencila:Note"
        },
        {
          "@id": "stencila:QuoteBlock"
        },
        {
          "@id": "schema:ListItem"
        },
        {
          "@id": "stencila:Form"
        },
        {
          "@id": "stencila:Span"
        },
        {
          "@id": "stencila:Heading"
        },
        {
          "@id": "stencila:Link"
        },
        {
          "@id": "stencila:Cite"
        },
        {
          "@id": "stencila:Mark"
        },
        {
          "@id": "stencila:IfClause"
        },
        {
          "@id": "schema:Claim"
        },
        {
          "@id": "schema:CreativeWork"
        },
        {
          "@id": "stencila:For"
        },
        {
          "@id": "stencila:Division"
        },
        {
          "@id": "stencila:TableCell"
        }
      ],
      "schema:rangeIncludes": {
        "@id": "stencila:Block"
      }
    }
  ]
}